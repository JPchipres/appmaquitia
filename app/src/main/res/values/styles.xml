<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="edittextStyle">
        <item name="android:textSize">20dp</item>
        <item name="android:background">@drawable/edittext_details</item>
    </style>
    <style name="edittextStyle2">
        <item name="android:textSize">20dp</item>
        <item name="android:background">@drawable/edittext2_details</item>
    </style>
    <style name="textStyle" parent="TextAppearance.AppCompat.Title">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="AlertDialogCustomFailed" parent="Theme.AppCompat.Light.Dialog.Alert">
    <!-- Otros atributos del estilo si es necesario -->
        <item name="android:fontFamily">@font/urbanist</item>
        <item name="android:textAlignment">center</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:background">@drawable/alert_failed</item>
    </style>
    <style name="AlertDialogCustomSuccess" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Otros atributos del estilo si es necesario -->
        <item name="android:fontFamily">@font/urbanist</item>
        <item name="android:textAlignment">center</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:background">@drawable/alert_success</item>
    </style>
    <style name="AlertDialogCustomWarning" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Otros atributos del estilo si es necesario -->
        <item name="android:fontFamily">@font/urbanist</item>
        <item name="android:textAlignment">center</item>
        <item name="android:background">@drawable/alert_warning</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
    </style>
    <style name="AlertDialogTerms" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@drawable/dialog_terms</item>
        <item name="android:fontFamily">@font/urbanist_semibold</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>